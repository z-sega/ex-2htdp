#reader(lib "htdp-beginner-reader.ss" "lang")((modname ex-129) (read-case-sensitive #t) (teachpacks ()) (htdp-settings #(#t constructor repeating-decimal #f #t none #f () #f)))

;; ex-129
;; Create BSL lists that represent
;; 1. a list of celestial bodies, say, at least all the
;; planets in our solar system.
;; 2. a list of items for a meal, for example, steak,
;; french fries, beans, bread, water, Brie cheese, and
;; ice cream; and
;; 3. a list of colors

;; 1.
(define CELESTIAL-BODIES
  (cons "Mercury"
        (cons "Venus"
              (cons "Earth"
                    (cons "Mars"
                          (cons "Jupiter"
                                (cons "Saturn"
                                      (cons "Uranus"
                                            (cons "Neptune"
                                                  '())))))))))

;; 2.
(define MEAL-PREP-ITEMS
  (cons "Steak"
        (cons "French fries"
              (cons "Beans"
                    (cons "Bread"
                          (cons "Water"
                                (cons "Brie Cheese"
                                      (cons "Ice cream"
                                            '()))))))))

;; 3.
(define COLORS
  (cons "red"
        (cons "green"
              (cons "blue"
                    (cons "yellow"
                          (cons "orange"
                                (cons "purple"
                                      '())))))))